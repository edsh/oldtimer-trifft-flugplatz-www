RewriteEngine On

Header always set Strict-Transport-Security "max-age=63072000; includeSubDomains; preload" "expr=%{HTTPS} == 'on'"

# non SSL
RewriteCond %{HTTPS} off
RewriteRule (.*) https://%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

# Everything not www.oldtimer-trifft-flugplatz.de
RewriteCond %{HTTP_HOST} !^www\.oldtimer-trifft-flugplatz\.de$ [NC]
RewriteRule ^(.*)$ https://www.oldtimer-trifft-flugplatz.de/$1 [L,R=301]
